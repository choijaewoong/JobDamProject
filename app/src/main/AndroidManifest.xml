<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidchoi.jobdam" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".Manager.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Calendar.SampleCalenarActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JobDetailActivity"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".CardWriteActivity"
                  android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".LogIn.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar.Splash" >
        </activity>
        <activity
            android:name=".LogIn.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".Setting.SettingActivity"
                  android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".Setting.HelpDeskActivity"
                  android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".Setting.PrivacyActivity"
                  android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".Setting.LegalActivity"
                  android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".Setting.AlarmSettingActivity"
                  android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".Setting.VersionInfoActivity"
                  android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
