<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                             android:layout_width="match_parent"
                             android:layout_height="match_parent"
                             android:orientation="vertical">


<RelativeLayout
    android:id="@+id/linearLayout1"
    android:layout_width="match_parent"
    android:layout_height="@dimen/calendar_title_height"
    android:orientation="horizontal"
    android:background="@android:color/white"
    android:layout_alignParentLeft="true"
    android:layout_alignParentTop="true"
    >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/image_prev_month_button"
        android:src="@drawable/button_calendar_left"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingLeft="@dimen/default_dimen"
        android:paddingRight="@dimen/default_dimen"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/image_next_month_button"
        android:src="@drawable/button_calendar_right"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:paddingRight="@dimen/default_dimen"
        android:paddingLeft="@dimen/default_dimen"/>

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_gravity="center_vertical"
        android:gravity="center_horizontal"
        android:textColor="@color/colorPrimary"
        android:textSize="@dimen/font_size_semi_large"
        android:textStyle="bold"/>

</RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/divider_width"
        android:layout_gravity="center_horizontal"
        android:background="@color/colorDivider"
        android:layout_below="@+id/linearLayout1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imageView34"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:paddingLeft="@dimen/default_dimen"
        android:paddingRight="@dimen/default_dimen"
        android:background="@android:color/white"
        android:layout_below="@+id/imageView34"
        android:id="@+id/relativeLayout7"
        android:gravity="center_vertical|right">


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView26"
            android:src="@drawable/image_calendar_header_start"
            android:layout_toLeftOf="@+id/textView12"
            android:layout_centerVertical="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="시작"
            android:id="@+id/textView12"
            android:layout_toLeftOf="@+id/imageView27"
            android:layout_marginRight="@dimen/default_small_dimen"
            android:layout_marginLeft="2dp"
            android:textSize="@dimen/font_size_min"
            android:textColor="@android:color/black"
            android:layout_centerInParent="true"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView27"
            android:src="@drawable/image_calendar_header_end"
            android:layout_toLeftOf="@+id/textView29"
            android:layout_centerVertical="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="마감"
            android:id="@+id/textView29"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="2dp"
            android:textSize="@dimen/font_size_min"
            android:textColor="@android:color/black"
            android:textIsSelectable="false"/>
    </RelativeLayout>

    <ImageView
    android:layout_width="match_parent"
    android:layout_height="@dimen/divider_width"
    android:layout_gravity="center_horizontal"
    android:background="@color/colorDivider"
    android:layout_below="@+id/relativeLayout7"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:id="@+id/imageView35"/>

    <LinearLayout
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="@dimen/calendar_header_height"
    android:layout_gravity="center_horizontal"
    android:background="@color/colorBackground"
    android:id="@+id/linearLayout10"
    android:layout_below="@+id/imageView35"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true">

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:text="일"
        android:id="@+id/textView7"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="@color/colorCalendarSun"
        android:textSize="@dimen/font_size_min"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:text="월"
        android:id="@+id/textView"
        android:layout_weight="1"
        android:gravity="center"
        android:textSize="@dimen/font_size_min"
        android:textColor="@color/colorCalendarInMonth"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:text="화"
        android:id="@+id/textView2"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="@color/colorCalendarInMonth"
        android:textSize="@dimen/font_size_min"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:text="수"
        android:id="@+id/textView3"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="@color/colorCalendarInMonth"
        android:textSize="@dimen/font_size_min"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:text="목"
        android:id="@+id/textView4"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="@color/colorCalendarInMonth"
        android:textSize="@dimen/font_size_min"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:text="금"
        android:id="@+id/textView5"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="@color/colorCalendarInMonth"
        android:textSize="@dimen/font_size_min"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:text="토"
        android:id="@+id/textView6"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="@color/colorCalendarSat"
        android:textSize="@dimen/font_size_min"/>

</LinearLayout>

<ImageView
    android:layout_width="match_parent"
    android:layout_height="@dimen/divider_width"
    android:layout_gravity="center_horizontal"
    android:background="@color/colorDivider"
    android:layout_below="@+id/linearLayout10"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:id="@+id/imageView36"/>

<GridView
    android:id="@+id/grid_view_calendar"
    android:listSelector="@color/colorPrimary"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/colorDivider"
    android:horizontalSpacing="@dimen/calendar_grid_separator"
    android:numColumns="7"
    android:stretchMode="columnWidth"
    android:verticalSpacing="@dimen/calendar_grid_separator"
    android:visibility="visible"
    android:layout_below="@+id/imageView36"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true">
</GridView>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/shadow_depth"
        android:id="@+id/imageView11"
        android:background="@drawable/shadow_bottom"
        android:visibility="visible"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false"
        android:layout_below="@+id/grid_view_calendar"/>

</RelativeLayout>