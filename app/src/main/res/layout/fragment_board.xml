    <com.example.androidchoi.jobdam.Util.CustomViewPager
        android:id="@+id/tab_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_weight="1"
        android:layout_alignParentTop="true"/>

    <View
        android:id="@+id/toolbar_shadow"
        android:layout_width="match_parent"
        android:layout_height="@dimen/shadow_depth"
        android:background="@drawable/shadow_bottom"/>

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/fab_write_article"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/button_write"
        app:fab_colorNormal="@color/colorPrimary"
        app:fab_colorPressed="@color/colorLightPrimary"
        app:fab_colorRipple="@color/colorPrimary"
        app:fab_label="@string/add_cartegory"
        app:fab_size="normal"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="@dimen/default_small_dimen"
        android:layout_marginBottom="@dimen/default_small_dimen"/>

</RelativeLayout>

